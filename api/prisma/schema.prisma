generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id          Int     @id @default(autoincrement())
  name        String
  email       String  @unique
  photo       String
  password    String
  role        String
  phoneNumber String
  birth       DateTime
}
